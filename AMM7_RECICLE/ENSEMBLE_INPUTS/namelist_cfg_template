!-----------------------------------------------------------------------
&nambfr        !   bottom friction
!-----------------------------------------------------------------------
  ln_bfr2d    = .false.
  ln_bfrimp   = .true.
  ln_loglayer = .true.
  nn_bfr      = 2
  rn_bfeb2    = 0.0
  rn_bfri1    = 4.0e-4
  rn_bfri2    = 1.0e-3
  rn_bfrz0    = 0.003
/
!-----------------------------------------------------------------------
&nambbc        !   bottom temperature boundary condition
!-----------------------------------------------------------------------
  ln_trabbc   = .false.    !  Apply a geothermal heating at the ocean bottom
